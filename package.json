{
  "name": "gulp-livescript",
  "version": "3.0.1",
  "description": "Compile livescript to JavaScript for Gulp",
  "main": "lib/index.js",
  "scripts": {
    "build": "lsc --no-header -b -c -o ./lib ./src/*",
    "dev": "mocha test/main.ls --require should --require livescript --watch",
    "dev_install": "npm install && npm dedupe",
    "test:cov": "cross-env NODE_ENV=test nyc cover _mocha --report lcov -- $npm_package_config_mocha",
    "test:watch": "npm test -- --watch",
    "test": "cross-env NODE_ENV=test mocha $npm_package_config_mocha",
    "prerelease": "npm run build && git add -A && git commit -m 'chore(lib): compile from src using lsc'",
    "release": "standard-version"
  },
  "config": {
    "mocha": "--compilers ls:livescript test/main.ls --require should"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomchentw/gulp-livescript"
  },
  "keywords": [
    "livescript",
    "gulp",
    "gulpplugin"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Atlas48/gulp-livescript-fork/issues"
  },
  "homepage": "https://github.com/Atlas48/gulp-livescript-fork",
  "dependencies": {
    "livescript": "^1.6.0",
    "nyc": "^15.1.0",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.1",
    "vinyl-sourcemaps-apply": "^0.2.1",
    "livescript": "^1.6.0"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "cross-env": "^7.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "mocha": "^8.0.1",
    "mocha-jscs": "^5.0.1",
    "mocha-jshint": "^2.3.1",
    "should": "^13.2.3",
    "standard-version": "^8.0.0"
  }
}
